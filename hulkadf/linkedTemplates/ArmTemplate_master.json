{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "hulkadf"
		},
		"AzureDataLakeStorageHulk_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageHulk'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"Hulk_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Hulk_SQL'"
		},
		"fargham_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'fargham_sql'"
		},
		"AzureDataLakeStorageHulk_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hulklakestorage.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageHulk_accountKey": {
						"value": "[parameters('AzureDataLakeStorageHulk_accountKey')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"Hulk_SQL_connectionString": {
						"value": "[parameters('Hulk_SQL_connectionString')]"
					},
					"fargham_sql_connectionString": {
						"value": "[parameters('fargham_sql_connectionString')]"
					},
					"AzureDataLakeStorageHulk_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageHulk_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}